pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'echo \'Building it\''
      }
    }

    stage('Test') {
      steps {
        echo 'echo \'Testing it!!!\''
      }
    }

    stage('Archiving') {
      steps {
        echo 'echo \'Archiving\''
      }
    }

  }
}